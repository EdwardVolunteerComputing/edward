<?xml version="1.0" encoding="UTF-8"?>
    <Configuration status="WARN">
    <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
    <PatternLayout pattern="%d{yy/MM/dd HH:mm:ss} %-6p(%F:%L) - %m%n"/>
    </Console>
    </Appenders>
    <Loggers>
    <Root level="INFO">
    <AppenderRef ref="Console"/>
    </Root>
    </Loggers>
    </Configuration>